	</contribution>

	
	<!-- 編成の定義 -->
	<contribution type="train" id="{FreeTrain-N1K2J-KeiH-TrainHensei-KIHA82}">
		<company>旧日本国有鉄道</company>
		<type>キハ 82</type>
		<name>特急型気動車</name>
		<author>Kei</author>
		<description>全国の非電化区間での特急サービスアップのため導入された初の気動車特急。初期のキハ81型はボンネット車体であった。晩年は主に北海道・紀勢方面で活躍。
		</description>
		<price>100</price>
		<speed>medium</speed>
		
		<class name="freetrain.contributions.train.PatternTrainImpl"/>
		<config>
			<car char="＜" ref="{FreeTrain-N1K2J-KeiH-TrainImg-KIHA82-F}"/>
			<car char="＞" ref="{FreeTrain-N1K2J-KeiH-TrainImg-KIHA82-R}"/>
			<car char="Ｓ" ref="{FreeTrain-N1K2J-KeiH-TrainImg-KISI80}"/>
			<car char="Ｈ" ref="{FreeTrain-N1K2J-KeiH-TrainImg-KIHA80}"/>
			
			<composition>＜Ｈ＞</composition>
			<composition>＜ＨＨ＞</composition>
			<composition>＜ＨＳＨ＞</composition>
			<composition>＜ＨＨＳＨ＞</composition>
			<composition>＜ＨＨＨＳＨ＞</composition>
			<composition>＜ＨＨＨＨＳＨ＞</composition>
			<composition>＜ＨＨＨＳ＞ＨＨ＞</composition>
			<composition>＜ＨＨＨＳＨ＞ＨＨ＞</composition>
			<composition>＜ＨＨＨＨＳＨ＞ＨＨ＞</composition>
			<composition>＜ＨＨＳＨ＞ＨＨ＞ＨＨ＞</composition>
			<composition>＜ＨＨＨＳＨ＞＜ＨＨＳＨ＞</composition>
			<composition>＜ＨＨＨＳＨ＞＜ＨＨＨＳＨ＞</composition>
			<composition>＜ＨＨＨＳＨ＞＜ＨＨＳＨ＞Ｈ＞</composition>
			<composition>＜ＨＨＳＨ＞ＨＨ＞＜ＨＨＨＳＨ＞</composition>
		</config>
	</contribution>
	
	<contribution type="train" id="{FreeTrain-N1K2J-KeiH-TrainHensei-DC0001}">
		<company>旧日本国有鉄道</company>
		<type>一般型気動車</type>
		<name>気動車混合編成</name>
		<author>Kei</author>
		<description>一般型であるキハ20、30、45、40型および急行型であるキハ58型気動車などを混合した編成。雑多な車種と塗装が特徴。郵便車や荷物車を組み込むこともある。
		</description>
		<price>100</price>
		<speed>medium</speed>
		
		<class name="freetrain.contributions.train.PatternTrainImpl"/>
		<config>
<!--DC45一般-->		<car char="＜" ref="{FreeTrain-N1K2J-KeiH-TrainImg-KIHA45-1-F}"/>
<!--DC45一般-->		<car char="＞" ref="{FreeTrain-N1K2J-KeiH-TrainImg-KIHA45-1-R}"/>
<!--DC45首都-->		<car char="≦" ref="{FreeTrain-N1K2J-KeiH-TrainImg-KIHA45-2-F}"/>
<!--DC45首都-->		<car char="≧" ref="{FreeTrain-N1K2J-KeiH-TrainImg-KIHA45-2-R}"/>
<!--DC40首都-->		<car char="４" ref="{FreeTrain-N1K2J-KeiH-TrainImg-KIHA40}"/>
<!--DC30一般-->		<car char="３" ref="{FreeTrain-N1K2J-KeiH-TrainImg-KIHA30-1}"/>
<!--DC30首都-->		<car char="③" ref="{FreeTrain-N1K2J-KeiH-TrainImg-KIHA30-2}"/>
			
			<composition>３</composition>
			<composition>４＞</composition>
			<composition>≦＞４</composition>
			<composition>４＞＞③</composition>
			<composition>③＞３４≧</composition>
			<composition>＜＜③＞≦４</composition>
			<composition>４３＞≦＞③＞</composition>
			<composition>＜③＜４≦４３≧</composition>
		</config>
	</contribution>

</plug-in>