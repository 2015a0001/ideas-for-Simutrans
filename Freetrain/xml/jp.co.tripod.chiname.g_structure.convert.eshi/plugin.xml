		</structure>
		<price>400</price>
		<size>1,2</size>
		<height>1</height>
		<sprite origin="0,0" offset="16">
			<picture ref="{19F42929-9606-4cfb-A2F0-EFE323487DC6}" />
		</sprite>
		<population>
			<class name="freetrain.contributions.population.RestaurantPopulation"/>
